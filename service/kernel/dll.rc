  #define INTERNAL_FILE_NAME "daal_sequential\0"
#elif defined(_DAAL_THR_TBB)
  #define FILE_COMMENT "Parallel version.\0"
  #define ORIG_FILE_NAME "daal_thread.dll \0"
  #define INTERNAL_FILE_NAME "daal_thread\0"
