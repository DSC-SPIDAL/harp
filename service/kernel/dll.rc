            VALUE "FileDescription", PRODUCT_NAME_STR
            VALUE "Comments", FILE_COMMENT
            VALUE "InternalName", INTERNAL_FILE_NAME
            VALUE "OriginalFilename", ORIG_FILE_NAME
            VALUE "CompanyName", "Intel Corporation\0"
